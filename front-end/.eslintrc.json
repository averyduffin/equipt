{
	"env": {
		"es6": true,
		"browser": true
	},
	"parser": "babel-eslint",
	"extends": ["airbnb"],
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"react/jsx-indent": ["off"],
		"react/jsx-indent-props": ["off"],
		"import/order": ["off"],
		"import/no-extraneous-dependencies": ["off"],
		"import/prefer-default-export": ["off"],
		"react/forbid-prop-types": ["off"],
		"react/prop-types": ["off"],
		"react/jsx-one-expression-per-line": ["off"]
	}
}
